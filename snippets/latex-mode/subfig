# -*- mode: snippet; require-final-newline: nil -*-
# name: subfig
# key: subfig
# --
% You should use graphicx, caption and subfigure packages
\begin{figure}
  \centering
  \subfigure[$1]{
    \includegraphics[width=0.4\textwidth]{$2}
  }
  \subfigure[$3]{
    \includegraphics[width=0.4\textwidth]{$4}
  }
  \caption{$5}
\end{figure}
