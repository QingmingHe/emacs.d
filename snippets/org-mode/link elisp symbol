# -*- mode: snippet; require-final-newline: nil -*-
# name: link elisp symbol
# key: link elisp symbol
# --
[[help:${1:`
(helm-comp-read "Symbol name: "
                (let ((symbol-names
                       (loop for x being the symbols
                             if (boundp x)
                             collect (symbol-name x))))
                  (mapatoms
                   (lambda (x)
                     (when (fboundp x)
                       (add-to-list 'symbol-names (symbol-name x)))))
                  symbol-names))`}][$1]]